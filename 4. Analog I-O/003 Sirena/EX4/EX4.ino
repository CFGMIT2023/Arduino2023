//********** Includes *************************************************************



//********** Variables ************************************************************
const byte pito = 9;        
int tiempo = 500;


//********** Setup ****************************************************************
void setup()
{
  pinMode(pito, OUTPUT);   
}

//********** Loop *****************************************************************
void loop()
{
  tone(pito, 1000, 2000); 

  delay(tiempo);    

  tone(pito, 1250, 2000);  
  
  delay(tiempo);
}

