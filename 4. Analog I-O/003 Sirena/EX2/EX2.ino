//********** Variables ************************************************************
const byte pito = 9;        
int tiempo = 5000;
//********** Setup ****************************************************************
void setup()
{
  pinMode(pito, OUTPUT);  
}

//********** Loop *****************************************************************
void loop()

{
  tone(pito, 1000, 3000); 
  delay(tiempo); 
}

